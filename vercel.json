{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/app.py"
    },
    {
      "src": "/scrape",
      "dest": "/app.py"
    },
    {
      "src": "/upload",
      "dest": "/app.py"
    },
    {
      "src": "/",
      "dest": "/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "functions": {
    "app.py": {
      "maxDuration": 30
    }
  }
}